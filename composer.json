{
  "name": "tenth/my-total-comfort",
  "type": "library",
  "description": "Interact with Honeywell MyTotalComfortControl-compatible Thermostats.",
  "minimum-stability": "dev",
  "keywords": ["HVAC", "Honeywell", "IoT", "Home Automation"],
  "homepage": "https://github.com/TenthPres/MyTotalComfort",
  "license": "MIT",
  "authors": [
    {
      "name": "James Kurtz",
      "email": "jkrrv@jkrrv.org",
      "role": "Developer"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/TenthPres/MyTotalComfort"
    }
  ],
  "autoload": {
    "psr-4": {
      "Tenth\\": "src/",
      "Tenth\\MyTotalComfort\\": "src/"
    }
  },
  "archive": {
    "exclude": [
      ".idea/"
    ]
  },
  "require": {
    "guzzlehttp/guzzle": "^6.2"
  },
  "require-dev": {
    "phpunit/phpunit": "5.6.*",
    "evert/phpdoc-md" : "~0.1.1"
  }
}