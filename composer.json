{
  "name": "tenth/my-total-comfort",
  "type": "library",
  "description": "Interact with Honeywell Total Comfort Control thermostats and devices.",
  "minimum-stability": "dev",
  "keywords": ["HVAC", "Honeywell", "IoT", "Home Automation", "IIoT"],
  "homepage": "https://github.com/TenthPres/MyTotalComfort",
  "license": "MIT",
  "authors": [
    {
      "name": "James Kurtz",
      "email": "jkrrv@jkrrv.org",
      "role": "Developer"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/TenthPres/MyTotalComfort"
    }
  ],
  "autoload": {
    "psr-4": {
      "Tenth\\": "src/",
      "Tenth\\MyTotalComfort\\": "src/"
    }
  },
  "archive": {
    "exclude": [
      ".idea/"
    ]
  },
  "require": {
    "guzzlehttp/guzzle": "^6.2",
    "ext-json": "*",
    "php": ">=5.6"
  },
  "require-dev": {
    "skayo/phpdoc-md" : "~0.2.0",
    "phpunit/phpunit": ">=5.0",
    "phpcompatibility/php-compatibility": "*",
    "squizlabs/php_codesniffer": "3.*",
    "php-coveralls/php-coveralls": "^2.2"
  },
  "scripts": {
    "phpcs": "phpcs --standard=phpcs_ruleset.xml"
  },
  "prefer-stable": true
}
